import java.util.Scanner;

public class EvenOrOdd {

    static String EvenorOdd(int n){
     
        if(n%2==0){
            return "Even";
        }
        else{
            return "Odd";
        }
        
    }

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter n = ");
        int n=sc.nextInt();
        System.out.println(EvenorOdd(n));
    }


    
}
