import java.util.Scanner;

public class Q2_greatest_among3 {

    static int EorO(int a,int b,int c){
       if(a>b && a>c){
        return a;
       }
       else if(b>c){
        return b;
       }
       else{
        return c;
       }
        
    }

    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter a= ");
        int a=sc.nextInt();
         System.out.print("enter b= ");
        int b=sc.nextInt();
         System.out.print("enter c= ");
        int c=sc.nextInt();
        System.out.println(EorO(a,b,c));
    }


    
}
