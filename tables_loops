




import java.util.Scanner;

public class Q13_tables {

static int EorO(int m){
   int s=0;
  for(int i=1;i<=10;i++){
    System.out.print(m*i+" ");
    s=s+(m*i);
  }
  System.out.println();
  return s;

        


        }
   
    
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        System.out.print("enter n = ");
        int m=sc.nextInt();
        
        System.out.println(EorO(m));
    }


    
}



